{
  "extends": ["eslint:recommended", "plugin:import/errors", "prettier"],
  "parser": "@babel/eslint-parser",
  "rules": {
    "no-console": 0,
    "no-debugger": 0,
    "no-unused-vars": 1
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  }
}
